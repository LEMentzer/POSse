<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="posse.UserController">
   <children>
      <TabPane prefHeight="400.0" prefWidth="602.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Add User">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Label layoutX="52.0" layoutY="63.0" text="Username:" />
                        <Label layoutX="52.0" layoutY="136.0" text="Password:" />
                        <TextField fx:id="newUser" layoutX="230.0" layoutY="55.0" />
                        <TextField fx:id="newPassword" layoutX="230.0" layoutY="128.0" />
                        <Button fx:id="addUser" layoutX="390.0" layoutY="221.0" mnemonicParsing="false" onAction="#addUserAction" text="Add" />
                        <CheckBox fx:id="manager" layoutX="52.0" layoutY="229.0" mnemonicParsing="false" text="Is Manager" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Delete User">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Label layoutX="60.0" layoutY="90.0" text="Username:" />
                        <TextField fx:id="oldUser" layoutX="235.0" layoutY="82.0" />
                        <Button fx:id="deleteUser" layoutX="251.0" layoutY="203.0" mnemonicParsing="false" onAction="#deleteUserAction" text="Delete" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
